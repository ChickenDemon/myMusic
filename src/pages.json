{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "enablePullDownRefresh": true, //设置为true表示当前页面开启下拉刷新
        "backgroundTextStyle": "dark"
      }
    },
    {
      "path": "pages/index/components/find",
      "style": {
        "navigationStyle": "custom",
        "enablePullDownRefresh": true, //设置为true表示当前页面开启下拉刷新
        "backgroundTextStyle": "dark"
      }
    },
    {
      "path": "pages/index/components/songSquare",
      "style": {
        "enablePullDownRefresh": true,
        "navigationBarTitleText": "歌单广场"
      }
    },
    {
      "path": "pages/index/components/songList",
      "style": {
        "enablePullDownRefresh": true,
        "navigationBarTitleText": "歌单",
        "app-plus":{
          "scrollIndicator":"none" //当前页面不显示滚动条
        },
        "scrollIndicator": "none"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "scrollIndicator": "none",
    "app-plus":{
      "scrollIndicator":"none" //当前页面不显示滚动条
    }
  }
}
